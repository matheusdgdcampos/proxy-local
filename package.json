{
  "name": "ts-mock-proxy",
  "version": "1.0.0",
  "description": "Proxy interceptador com motor de mocks e dashboard",
  "main": "dist/start-dev.js",
  "scripts": {
    "start": "ts-node src/start-dev.ts",
    "build": "tsc && webpack --mode production",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/start-dev.ts",
    "generate-certs": "node scripts/generate-certs.js",
    "mock": "ts-node src/mocks/mock-server.ts",
    "lint": "biome check --write",
    "lint:ci": "biome check --write --staged"
  },
  "keywords": [
    "proxy",
    "mock",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "better-sqlite3": "^9.2.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "commitizen": "^4.3.1",
    "css-loader": "^6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "html-webpack-plugin": "^5.5.3",
    "lefthook": "^1.13.4",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
